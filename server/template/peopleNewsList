<script>
module.exports = {
  created: function () {
    this.changeItem(0)
    const newsList = owo.query('.qiehuan1 i')
    for (let ind = 0; ind < newsList.length; ind++) {
      newsList[ind].onmouseover = () => {
        this.changeItem(ind)
      }
    }
  },
  changeItem: function (index) {
    const menuList = owo.query('.qiehuan1 i')
    const newsList = owo.query('.headingNews')
    for (let ind = 0; ind < newsList.length; ind++) {
      newsList[ind].style.display = 'none'
    }
    for (let ind = 0; ind < menuList.length; ind++) {
      menuList[ind].classList.remove('one')
    }
    newsList[index].style.display = 'block'
    menuList[index].classList.add('one')
  }
}
</script>

